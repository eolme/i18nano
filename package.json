{
  "name": "i18nano",
  "version": "2.0.0",
  "source": "./src/index.ts",
  "main": "./lib/index.cjs",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "exports": {
    "require": "./lib/index.cjs",
    "import": "./lib/index.js",
    "types": "./lib/index.d.ts"
  },
  "author": {
    "name": "Anton Petrov",
    "email": "im@petrov.engineer",
    "url": "https://petrov.engineer"
  },
  "description": "Lightweight translation module with functionality similar to react-i18next",
  "license": "MIT",
  "keywords": [
    "i18n",
    "i18next",
    "internationalization",
    "l10n",
    "localization",
    "translation",
    "react",
    "reactjs"
  ],
  "homepage": "https://github.com/eolme/i18nano",
  "bugs": "https://github.com/eolme/i18nano/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/eolme/i18nano.git"
  },
  "sideEffects": false,
  "scripts": {
    "build": "nanobundle build",
    "test": "yarn node --experimental-modules --experimental-vm-modules --enable-source-maps $(yarn bin jest)",
    "example:nextjs": "cd examples/nextjs && yarn run next dev"
  },
  "workspaces": [
    "examples/*"
  ],
  "devDependencies": {
    "@jest/types": "^28.1.3",
    "@mntm/eslint-config": "^1.2.0",
    "@types/get-value": "^3.0.2",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.6.3",
    "@types/react": "^18.0.15",
    "@types/react-test-renderer": "^18.0.0",
    "eslint": "^8.21.0",
    "eslint-plugin-jest": "^26.7.0",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "jest": "^28.1.3",
    "jest-esbuild": "^0.2.9",
    "jest-mock": "^28.1.3",
    "jest-resolve": "^28.1.3",
    "jest-ts-webcompat-resolver": "^1.0.0",
    "nanobundle": "^0.0.28",
    "react": "^18.2.0",
    "react-reconciler": "^0.29.0",
    "react-test-renderer": "^18.2.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "get-value": "^3.0.1",
    "suspend-react": "^0.0.8"
  },
  "peerDependencies": {
    "react": ">=17.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": false
    }
  }
}
